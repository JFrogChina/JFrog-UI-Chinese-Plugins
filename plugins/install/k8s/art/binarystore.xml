<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config version="2">
    <chain template="s3-storage-v3-direct"/>
    <provider type="cache-fs" id="cache-fs">
        <maxCacheSize>50gb</maxCacheSize>
        <cacheProviderDir>/var/opt/jfrog/artifactory/data/artifactory/cache</cacheProviderDir>
    </provider>
    <provider type="s3-storage-v3" id="s3-storage-v3">
        <bucketName>jfrog-artifactory</bucketName>
        <path>artifactory/filestore</path>
        <endpoint>https://oss-cn-xxx.aliyuncs.com</endpoint>
        <identity>xxx</identity>
        <credential>xxx</credential>
        <testConnection>false</testConnection>
        <enableSignedUrlRedirect>false</enableSignedUrlRedirect>
        <region>cn-xxx</region>
        <transferManagerThreads>15</transferManagerThreads>
        <multiPartLimit>1000000000</multiPartLimit>
        <maxConnections>50</maxConnections>
    </provider>
</config>
